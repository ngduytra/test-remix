{
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/index.js": {
      "includeFiles": "build/**",
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
